FeedCard {
  padding-left: 1rem
  padding-bottom: .5rem
  border-left: 5px solid #fff
  filter: saturate(30%) opacity(50%)

  transition: all 0.1s ease-in

  :focus {
    outline: none
    border-left: 5px solid #dbdbdb
    filter: none
  }

  /* TODO remove this dep this will  */
  div.Message {
    :focus { outline: none }
  }

}

